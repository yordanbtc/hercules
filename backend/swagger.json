          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          },
          "type": {
            "type": "object",
            "default": "USER"
          }
        },
        "required": [
          "id",
          "email",
          "telephone",
          "firstName",
          "lastName",
          "createdAt",
          "updatedAt",
          "type"
        ]
      },
