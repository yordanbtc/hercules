        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "uuid-v4-123"
          },
          "name": {
            "type": "string",
            "example": "Nombre del Producto"
          },
          "price": {
            "type": "object",
            "example": 99.99
          },
          "stock": {
            "type": "number",
            "example": 10
          },
          "description": {
            "type": "string",
            "example": "Descripcion del roducto"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "price",
          "stock",
          "description",
          "createdAt"
        ]
      },
